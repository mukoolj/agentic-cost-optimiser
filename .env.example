# --- Cloud/AWS (or use OIDC in CI) ---
AWS_REGION=ap-southeast-2

# --- LLM ---
LLM_PROVIDER=openai
OPENAI_API_KEY=sk-...
LLM_MODEL=gpt-4o-mini

# --- Repo / PR ---
GIT_REMOTE=origin
GIT_MAIN_BRANCH=main
GIT_USER_NAME=agent-bot
GIT_USER_EMAIL=agent-bot@example.com

# --- Optimiser thresholds ---
LOOKBACK_DAYS=14
MIN_UTIL=0.15  # below this is candidate
BATCH_SIZE=5

# --- SLO / Guardrails ---
MAX_P95_LATENCY_MS=200
MAX_ERROR_RATE=0.01

# --- Freeze windows (cron-like or weekdays) ---
FREEZE_WINDOW_CRON=
